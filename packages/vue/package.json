{
  "name": "@ldesign/color-vue",
  "version": "2.0.0",
  "description": "Vue 3 components and composables for @ldesign/color-core",
  "keywords": [
    "color",
    "vue",
    "vue3",
    "theme",
    "composables",
    "components"
  ],
  "author": "ldesign",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./esm/index.d.ts",
      "import": "./esm/index.js",
      "require": "./cjs/index.cjs",
      "default": "./esm/index.js"
    },
    "./color-picker": {
      "types": "./esm/color-picker/index.d.ts",
      "import": "./esm/color-picker/index.js",
      "require": "./cjs/color-picker/index.cjs",
      "default": "./esm/color-picker/index.js"
    },
    "./color-picker/*": {
      "types": "./esm/color-picker/*.d.ts",
      "import": "./esm/color-picker/*.js",
      "require": "./cjs/color-picker/*.cjs",
      "default": "./esm/color-picker/*.js"
    },
    "./composables/*": {
      "types": "./esm/composables/*.d.ts",
      "import": "./esm/composables/*.js",
      "require": "./cjs/composables/*.cjs",
      "default": "./esm/composables/*.js"
    },
    "./devtools": {
      "types": "./esm/devtools/index.d.ts",
      "import": "./esm/devtools/index.js",
      "require": "./cjs/devtools/index.cjs",
      "default": "./esm/devtools/index.js"
    },
    "./devtools/*": {
      "types": "./esm/devtools/*.d.ts",
      "import": "./esm/devtools/*.js",
      "require": "./cjs/devtools/*.cjs",
      "default": "./esm/devtools/*.js"
    },
    "./palette-display": {
      "types": "./esm/palette-display/index.d.ts",
      "import": "./esm/palette-display/index.js",
      "require": "./cjs/palette-display/index.cjs",
      "default": "./esm/palette-display/index.js"
    },
    "./palette-display/*": {
      "types": "./esm/palette-display/*.d.ts",
      "import": "./esm/palette-display/*.js",
      "require": "./cjs/palette-display/*.cjs",
      "default": "./esm/palette-display/*.js"
    },
    "./plugin": {
      "types": "./esm/plugin/index.d.ts",
      "import": "./esm/plugin/index.js",
      "require": "./cjs/plugin/index.cjs",
      "default": "./esm/plugin/index.js"
    },
    "./plugin/*": {
      "types": "./esm/plugin/*.d.ts",
      "import": "./esm/plugin/*.js",
      "require": "./cjs/plugin/*.cjs",
      "default": "./esm/plugin/*.js"
    },
    "./plugins": {
      "types": "./esm/plugins/index.d.ts",
      "import": "./esm/plugins/index.js",
      "require": "./cjs/plugins/index.cjs",
      "default": "./esm/plugins/index.js"
    },
    "./plugins/*": {
      "types": "./esm/plugins/*.d.ts",
      "import": "./esm/plugins/*.js",
      "require": "./cjs/plugins/*.cjs",
      "default": "./esm/plugins/*.js"
    },
    "./ssr": {
      "types": "./esm/ssr/index.d.ts",
      "import": "./esm/ssr/index.js",
      "require": "./cjs/ssr/index.cjs",
      "default": "./esm/ssr/index.js"
    },
    "./ssr/*": {
      "types": "./esm/ssr/*.d.ts",
      "import": "./esm/ssr/*.js",
      "require": "./cjs/ssr/*.cjs",
      "default": "./esm/ssr/*.js"
    },
    "./theme-color-picker": {
      "types": "./esm/theme-color-picker/index.d.ts",
