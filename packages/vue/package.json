      "types": "./esm/theme-mode-switcher/index.d.ts",
      "import": "./esm/theme-mode-switcher/index.js",
      "require": "./cjs/theme-mode-switcher/index.cjs",
      "default": "./esm/theme-mode-switcher/index.js"
    },
    "./theme-mode-switcher/*": {
      "types": "./esm/theme-mode-switcher/*.d.ts",
      "import": "./esm/theme-mode-switcher/*.js",
      "require": "./cjs/theme-mode-switcher/*.cjs",
      "default": "./esm/theme-mode-switcher/*.js"
    },
    "./theme-provider": {
      "types": "./esm/theme-provider/index.d.ts",
      "import": "./esm/theme-provider/index.js",
      "require": "./cjs/theme-provider/index.cjs",
      "default": "./esm/theme-provider/index.js"
    },
    "./theme-provider/*": {
      "types": "./esm/theme-provider/*.d.ts",
      "import": "./esm/theme-provider/*.js",
      "require": "./cjs/theme-provider/*.cjs",
      "default": "./esm/theme-provider/*.js"
    },
    "./utils/*": {
      "types": "./esm/utils/*.d.ts",
      "import": "./esm/utils/*.js",
      "require": "./cjs/utils/*.cjs",
      "default": "./esm/utils/*.js"
    },
    "./dist/index.css": "./dist/index.css",
    "./dist/index.min.css": "./dist/index.min.css",
    "./package.json": "./package.json"
  },
  "main": "./cjs/index.cjs",
  "module": "./esm/index.js",
  "types": "./esm/index.d.ts",
  "unpkg": "./dist/index.min.js",
  "jsdelivr": "./dist/index.min.js",
  "files": [
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "dist"
  ],
  "scripts": {
    "build": "ldesign-builder build",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit",
    "clean": "node -e \"const fs=require('fs'); ['es','lib','dist','.tsbuildinfo'].forEach(d=>{try{fs.rmSync(d,{recursive:true,force:true})}catch{}})\""
  },
  "peerDependencies": {
    "@ldesign/color-core": "workspace:*",
    "vue": ">=3.0.0"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "typescript": "^5.7.3",
    "vue": "^3.0.0"
  },
  "typings": "./esm/index.d.ts",
  "browser": "./dist/index.js"
}