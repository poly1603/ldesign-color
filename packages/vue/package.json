{
  "name": "@ldesign/color-vue",
  "version": "1.0.0",
  "description": "Vue 3 components and composables for @ldesign/color-core",
  "keywords": [
    "color",
    "vue",
    "vue3",
    "theme",
    "composables",
    "components"
  ],
  "author": "ldesign",
  "license": "MIT",
  "type": "module",
  "sideEffects": [
    "*.css",
    "*.vue",
    "**/*.css",
    "**/style/**"
  ],
  "exports": {
    ".": {
      "types": "./esm/index.d.ts",
      "import": "./esm/index.js",
      "require": "./cjs/index.cjs"
    },
    "./color-picker": {
      "types": "./esm/color-picker/index.d.ts",
      "import": "./esm/color-picker/index.js",
      "require": "./cjs/color-picker/index.cjs"
    },
    "./color-picker/*": {
      "types": "./esm/color-picker/*.d.ts",
      "import": "./esm/color-picker/*.js",
      "require": "./cjs/color-picker/*.cjs"
    },
    "./components/*": {
      "types": "./esm/components/*.d.ts",
      "import": "./esm/components/*.js",
      "require": "./cjs/components/*.cjs"
    },
    "./composables/*": {
      "types": "./esm/composables/*.d.ts",
      "import": "./esm/composables/*.js",
      "require": "./cjs/composables/*.cjs"
    },
    "./palette-display": {
      "types": "./esm/palette-display/index.d.ts",
      "import": "./esm/palette-display/index.js",
      "require": "./cjs/palette-display/index.cjs"
    },
    "./palette-display/*": {
      "types": "./esm/palette-display/*.d.ts",
      "import": "./esm/palette-display/*.js",
      "require": "./cjs/palette-display/*.cjs"
    },
    "./plugin": {
      "types": "./esm/plugin/index.d.ts",
      "import": "./esm/plugin/index.js",
      "require": "./cjs/plugin/index.cjs"
    },
    "./plugin/*": {
      "types": "./esm/plugin/*.d.ts",
      "import": "./esm/plugin/*.js",
      "require": "./cjs/plugin/*.cjs"
    },
    "./plugins": {
      "types": "./esm/plugins/index.d.ts",
      "import": "./esm/plugins/index.js",
      "require": "./cjs/plugins/index.cjs"
    },
    "./plugins/*": {
      "types": "./esm/plugins/*.d.ts",
      "import": "./esm/plugins/*.js",
      "require": "./cjs/plugins/*.cjs"
    },
    "./theme-color-picker": {
      "types": "./esm/theme-color-picker/index.d.ts",
      "import": "./esm/theme-color-picker/index.js",
      "require": "./cjs/theme-color-picker/index.cjs"
    },
    "./theme-color-picker/*": {
      "types": "./esm/theme-color-picker/*.d.ts",
      "import": "./esm/theme-color-picker/*.js",
      "require": "./cjs/theme-color-picker/*.cjs"
    },
    "./theme-mode-switcher": {
      "types": "./esm/theme-mode-switcher/index.d.ts",
      "import": "./esm/theme-mode-switcher/index.js",
      "require": "./cjs/theme-mode-switcher/index.cjs"
    },
    "./theme-mode-switcher/*": {
      "types": "./esm/theme-mode-switcher/*.d.ts",
      "import": "./esm/theme-mode-switcher/*.js",
      "require": "./cjs/theme-mode-switcher/*.cjs"
    },
    "./theme-provider": {
      "types": "./esm/theme-provider/index.d.ts",
      "import": "./esm/theme-provider/index.js",
      "require": "./cjs/theme-provider/index.cjs"
    },
    "./theme-provider/*": {
      "types": "./esm/theme-provider/*.d.ts",
      "import": "./esm/theme-provider/*.js",
      "require": "./cjs/theme-provider/*.cjs"
    },
    "./cjs/color-picker/index.css": "./cjs/color-picker/index.css",
    "./cjs/components/ThemeColorPicker.css": "./cjs/components/ThemeColorPicker.css",
    "./cjs/components/ThemeModeSwitcher.css": "./cjs/components/ThemeModeSwitcher.css",
    "./cjs/index.css": "./cjs/index.css",
    "./cjs/palette-display/index.css": "./cjs/palette-display/index.css",
    "./cjs/theme-provider/index.css": "./cjs/theme-provider/index.css",
    "./dist/index.css": "./dist/index.css",
    "./dist/index.min.css": "./dist/index.min.css"
  },
  "main": "./cjs/index.cjs",
  "module": "./esm/index.js",
  "types": "./esm/index.d.ts",
  "unpkg": "./dist/index.min.js",
  "jsdelivr": "./dist/index.min.js",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "esm",
    "cjs",
    "dist"
  ],
  "scripts": {
    "build": "ldesign-builder build",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit",
    "clean": "node -e \"const fs=require('fs'); ['es','lib','dist','.tsbuildinfo'].forEach(d=>{try{fs.rmSync(d,{recursive:true,force:true})}catch{}})\""
  },
  "peerDependencies": {
    "@ldesign/color-core": "workspace:*",
    "vue": ">=3.0.0"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "typescript": "^5.7.3",
    "vue": "^3.0.0"
  }
}